{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^db:generate"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "db:generate": {
      "outputs": [
        "prisma/client/**"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "format": {},
    "format:fix": {},
    "dev": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false,
      "persistent": true
    }
  }
}
